create or replace function Array69 (num in number)
return number
as
e_edgecase exception;
l_low pls_integer := 1;
l_high pls_integer;
l_mid pls_integer;
begin
if(num < 1) then
raise e_edgecase;
end if;
l_high := num;
while (l_low < l_high)
loop
l_mid := (l_low + l_high )/2;
if(l_mid <= (num/l_mid)) then
l_low := l_mid + 1;
RETURN l_mid;
else
l_high := l_mid - 1;
end loop;
exception
when e_edgecase
then
dbms_output.put_line('Edge case..!');
when others
then
dbms_output.put_line(dbms_utility.format_error_backtrace());
dbms_output.put_line(SQLCODE || SQLERRM);
end;
